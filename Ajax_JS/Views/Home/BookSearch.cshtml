@model IEnumerable<Ajax_JS.Models.Book>

<div id="searchresults">
    @if (Model.Count() > 0)
    {
        <h3>Все книги автора : @Model.First().Author</h3>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name</li>
            }
        </ul>
    }
</div>

